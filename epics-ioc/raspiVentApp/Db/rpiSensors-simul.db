record(calc,"$(P)$(R)Sensor-Pressure"){
    field(DESC,"Input Air pressure ")
    field(PREC,"3")
    field(INPA, "$(P)$(R)TIME-COUNTER")
    field(CALC, "(A>=1.0)? 0 : -A")
    field(EGU,"cmH2O")
    field(HOPR,"10")
    field(LOPR,"0")
    field(HIHI,"10")
    field(HIGH,"8")
    field(LOW,"2")
    field(LOLO,"1")
}
