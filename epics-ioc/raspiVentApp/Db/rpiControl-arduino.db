record(longout,"$(P)$(R)Set-Vinsp"){
    field(DESC,"Set Inspiratory Valve")
    field(EGU,"")
    field(VAL,"0")
    field(DRVH,"255")
    field(DRVL,"0")
    field(HOPR,"255")
    field(LOPR,"0")
    field (DTYP, "stream")
    field (OUT, "@arduino.proto SendTargetVinsp $(PORT)")
    # field(FLNK, "$(P)$(R)MessageOut")
}
record(longout,"$(P)$(R)Set-Vexp"){
    field(DESC,"Set Expiratory Valve")
    field(EGU,"")
    field(VAL,"0")
    field(DRVH,"1")
    field(DRVL,"0")
    field(HOPR,"1")
    field(LOPR,"0")
    # field(FLNK, "$(P)$(R)MessageOut")
}

record(longout,"$(P)$(R)Set-InspExpState"){
    field(DESC,"Set Inspiration/Experiation") #0,1,2 -> idle, Insp, Exp
    field(EGU,"")
    field(VAL,"0")
    field(DRVH,"2")
    field(DRVL,"0")
    field(HOPR,"2")
    field(LOPR,"0")
}
