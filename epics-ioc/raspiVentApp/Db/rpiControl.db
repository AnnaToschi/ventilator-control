record(sel, "$(P)$(R)Sel-Pinsp") {
    field(DESC, "Selection of source to P-Insp valve")
    field(SELM,"Specified")
    field(NVL,"$(P)$(R)OPMODE.RVAL CP NMS")

    field(INPA, "0")
    field(INPB, "0")
    field(INPC, "$(P)$(R)Ref-Pinsp-PC") #  VC
    field(INPD, "$(P)$(R)Ref-Pinsp-PC CP NMS") # PC
    field(INPE, "0") # PS
}
